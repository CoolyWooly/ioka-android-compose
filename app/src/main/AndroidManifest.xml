<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kz.ioka.android.iokademoapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".iokaDemoApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.IokaDemoApp">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="kz.ioka.android.iokademoapp.presentation.cart.orderDetail.OrderDetailsActivity" />
        <activity android:name="kz.ioka.android.iokademoapp.presentation.cart.paymentType.SelectPaymentTypeActivity" />
        <activity android:name="kz.ioka.android.iokademoapp.presentation.profile.language.SelectLanguageActivity" />
        <activity android:name="kz.ioka.android.iokademoapp.presentation.profile.savedCards.SavedCardsActivity" />
    </application>

</manifest>